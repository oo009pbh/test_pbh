#include <iostream>
#include <vector>
using namespace std;

int main()
{
	long long n,b,c,i;
	vector<long long> a;
	long long sum_supervisor = 0;
	cin >> n;

	a.resize(n);
	sum_supervisor += n; // 총감독관 수 미리 더하기
	
	for (i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	
	cin >> b >> c;
	
	for (i = 0; i < n; i++) // 
	{
		if (a[i] - b <= 0) // 총감독관 수로 다 커버 가능한지 검사
		{
			;
		}
		else if ((a[i] - b) % c == 0) // 각 시험장의 인원에 총감독관이 감시가능한 수 빼고 나머지를 c 로 나눠 필요한 부 감독관 수 계산하기
		{	                          // 총감독관 인원이 딱 떨어지면 그대로 더하기
			sum_supervisor += (a[i] - b) / c;
		}
		else // 딱떨어지지 않으면 부감독관 한명 더 더하기
		{
			sum_supervisor += (a[i] - b) / c  +  1;
		}
			
	}

	cout << sum_supervisor;
	return 0;
}
